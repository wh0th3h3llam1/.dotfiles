{
	"bracket-pair-colorizer-2.colorMode": "Consecutive",
	"bracket-pair-colorizer-2.colors": [
		"Orchid",
		"Gold",
		"Teal",
		"LightSkyBlue",
		"Green",
		"Lime",
		"Yellow",
		"Orange",
		"White",
	],
	"bracket-pair-colorizer-2.excludedLanguages": [
		"HTML"
	],
	"bracket-pair-colorizer-2.activeScopeCSS": [
		"borderStyle : solid",
		"borderWidth : 1px",
		"borderColor : {color}",
		"opacity: 0.5"
	],
	"bracket-pair-colorizer-2.forceIterationColorCycle": true,
	"bracket-pair-colorizer-2.forceUniqueOpeningColor": true,
	"bracket-pair-colorizer-2.highlightActiveScope": true,
	"bracket-pair-colorizer-2.showBracketsInGutter": true,
	
	"debug.openExplorerOnEnd": true,
	"debug.saveBeforeStart": "nonUntitledEditorsInActiveGroup",
	"diffEditor.ignoreTrimWhitespace": false,
	"djangointellisense.settingsModule": "prosit.settings",
	
	"editor.autoClosingBrackets": "always",
	"editor.bracketPairColorization.independentColorPoolPerBracketType": false,
	"editor.fontSize": 17,
	"editor.fontLigatures": true,
	"editor.formatOnSave": true,
	"editor.guides.bracketPairs": true,
	"editor.guides.bracketPairsHorizontal": true,
	"editor.linkedEditing": true,
	"editor.matchBrackets": "never",
	"editor.tokenColorCustomizations": {
		"[Material Monokai High Contrast]": {
			"textMateRules": [
				{
					// Python Function Call
					"name": "Python Generic Function Call",
					"scope": "meta.function-call.generic.python",
					"settings": {
						"foreground": "#A6E22E",
						"fontStyle": "italic bold"
					}
				},
				{
					// Python Attributes
					"name": "Python Attributes",
					"scope": "meta.attribute.python",
					"settings": {
						"foreground": "#1EDBEC",
						"fontStyle": "underline"
					}
				},
				{
					// Python Class Name
					"name": "Python Class Name",
					"scope": "entity.name.type.class.python",
					"settings": {
						"foreground": "#9DFF00",
						"fontStyle": ""
					}
				},
			],
			// "variables": "#ff8c00"
		},
		"[Monokai]": {
			"textMateRules": [
				{
					// All CAPS Constant
					"scope": "constant.other.caps.python",
					"settings": {
						"foreground": "#B78AFF"
					}
				},
				{
					// Python Attributes
					"scope": "meta.attribute.python",
					"settings": {
						"foreground": "#1EDBEC",
						"fontStyle": "underline"
					}
				},
				
			]
		},
	},
	"editor.wordWrap": "on",
	"explorer.confirmUndo": "verbose",
	
	"files.insertFinalNewline": true,

	"git.autofetch": true,

	"html.customData": [
		"./.vscode/html-data.json"
	],
	"html.format.indentHandlebars": true,
	"html.format.indentInnerHtml": true,

	"pylance.insidersChannel": "daily",
	"python.formatting.provider": "autopep8",
	"python.languageServer": "Pylance",
	"python.linting.enabled": true,
	"python.linting.pylintEnabled": true,
	"python.linting.pycodestyleArgs": [
		"--ignore=E501"
	],
	"python.linting.lintOnSave": true,
	"python.linting.pylintArgs": [
		// "--disable=W0311",
		"--load-plugin=pylint_django"
	],
	"python.linting.pylintCategorySeverity.warning": "Information",
	
	"tabnine.experimentalAutoImports": true,
	"telemetry.telemetryLevel": "off",
	"terminal.integrated.commandsToSkipShell": [
		"language-julia.interrupt"
	],
	"terminal.integrated.defaultProfile.windows": "Command Prompt",
	"typescript.updateImportsOnFileMove.enabled": "always",
	
	"workbench.activityBar.visible": true,
	"workbench.colorTheme": "Monokai",
	"workbench.editorAssociations": {
		"*.pdf": "default",
		"*.ipynb": "jupyter-notebook"
	},
	"workbench.iconTheme": "material-icon-theme",
	"workbench.productIconTheme": "material-product-icons",
	"workbench.colorCustomizations": {
		"[Material Monokai High Contrast]": {
			"editor.background": "#12181B",
			"editorGutter.addedBackground": "#487E02",
			"editorGutter.deletedBackground": "#C20000",
			"editorGutter.modifiedBackground": "#1E96C5",
			"gitDecoration.addedResourceForeground": "#00CCCC",
			"gitDecoration.ignoredResourceForeground": "#777777",
			"gitDecoration.modifiedResourceForeground": "#DAB678",
			"gitDecoration.renamedResourceForeground": "#E98F28",
			"gitDecoration.deletedResourceForeground": "#ff2020",
			"gitDecoration.stageDeletedResourceForeground": "#DC3A0D",
			"gitDecoration.stageModifiedResourceForeground": "#0FD7FF",
			"gitDecoration.untrackedResourceForeground": "#90C356",
			"gitlens.decorations.ignoredForegroundColor": "#777777",
			"sideBar.foreground": "#BEBEBE",
			"sideBar.background": "#161F23",
			"statusBar.debuggingBackground": "#DD6819",
			"statusBar.foreground": "#CCCCCC",
			// "statusBar.background": "#007ACC",
			"symbolIcon.classForeground": "#9DFF00",
		},
		"[Monokai]": {
			"editor.background": "#1D1E1B",
			"statusBar.background": "#2A2C2B",
			"statusBar.debuggingBackground": "#DD6819",
		},
		"editorBracketHighlight.foreground1": "#FFD700",
		"editorBracketHighlight.foreground2": "#00CED1",
		"editorBracketHighlight.foreground3": "#3CB371",
		"editorBracketHighlight.foreground4": "#ADFF2F",
		"editorBracketHighlight.foreground5": "#FF8C00",
		"editorBracketHighlight.foreground6": "#FFFFFF"
	},
	"workbench.startupEditor": "none",
	
	"[css]": {
		"editor.defaultFormatter": "aeschli.vscode-css-formatter"
	},
	"[python]": {
		"editor.defaultFormatter": "ms-python.black-formatter"
	},
	"thunder-client.codeSnippetLanguage": "cs-httpclient",
	
    // For Custom folder icons in Material Icon Theme
    // Place icons in %USERPROFILE%\.vscode\extensions\icons\
    // Name should be in the format:
    // folder-<serializer>.svg
    // folder-<serializer>-open.svg
    "material-icon-theme.folders.associations": {
        "serializer": "serializer",
        "serializers": "serializer"
    },
}
