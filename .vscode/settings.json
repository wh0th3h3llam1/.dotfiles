{
    "better-comments.highlightPlainText": true,
    "black-formatter.showNotifications": "onWarning",
    "bracket-pair-colorizer-2.colorMode": "Consecutive",
    "bracket-pair-colorizer-2.colors": [
        "Gold",
        "Teal",
        "LightSkyBlue",
        "Green",
        "Lime",
        "Yellow",
        "Orange",
        "White",
    ],
    "bracket-pair-colorizer-2.excludedLanguages": [
        "HTML"
    ],
    "bracket-pair-colorizer-2.activeScopeCSS": [
        "borderStyle : solid",
        "borderWidth : 1px",
        "borderColor : {color}",
        "opacity: 0.5"
    ],
    "bracket-pair-colorizer-2.depreciation-notice": false,
    "bracket-pair-colorizer-2.forceIterationColorCycle": true,
    "bracket-pair-colorizer-2.forceUniqueOpeningColor": true,
    "bracket-pair-colorizer-2.highlightActiveScope": true,
    "bracket-pair-colorizer-2.showBracketsInGutter": true,

    "debug.openExplorerOnEnd": true,
    "debug.saveBeforeStart": "nonUntitledEditorsInActiveGroup",
    "doki.sticker.path": "F:\\HD Wallpapers\\sasuke.webp",
    "doki.sticker.css": "z-index:100;background-position:100% 97%;background-size: 7.5%",
    "doki.statusbar.name": "Zero",
    "doki.background.enabled": true,
    "doki.background.path": "F:\\HD Wallpapers\\Anime\\Naruto\\uchiha-mangekyo.jpg",
    "doki.wallpaper.enabled": false,
    "doki.wallpaper.path": "F:\\HD Wallpapers\\Anime\\Naruto\\uchiha-mangekyo.jpg",

    "editor.accessibilitySupport": "off",
    "editor.autoClosingBrackets": "always",
    // Controls whether bracket pair colorization is enabled or not. Use 'workbench.colorCustomizations' to override the bracket highlight colors.
	"editor.bracketPairColorization.enabled": true,
    "editor.cursorBlinking": "smooth",
    "editor.detectIndentation": false,
    "editor.fontSize": 17,
    "editor.fontLigatures": true,
    // Controls whether the editor should render indent guides.
	"editor.guides.indentation": true,
	// Controls whether bracket pair guides are enabled or not.
	//  - true: Enables bracket pair guides.
	//  - active: Enables bracket pair guides only for the active bracket pair.
	//  - false: Disables bracket pair guides.
	"editor.guides.bracketPairs": "active",
	// Controls whether horizontal bracket pair guides are enabled or not.
	//  - true: Enables horizontal guides as addition to vertical bracket pair guides.
	//  - active: Enables horizontal guides only for the active bracket pair.
	//  - false: Disables horizontal bracket pair guides.
	"editor.guides.bracketPairsHorizontal": "active",
	// Controls whether the editor should highlight the active indent guide.
	"editor.guides.highlightActiveIndentation": true,
	// Controls whether bracket pair guides are enabled or not.
	"editor.guides.highlightActiveBracketPair": true,
    "editor.linkedEditing": true,
    // Defines the bracket symbols that increase or decrease the indentation.
	"editor.language.brackets": [
		["[", "]"],
		["(", ")"],
		["{", "}"]
	],
    // Defines the bracket pairs that are colorized by their nesting level if bracket pair colorization is enabled.
    "editor.language.colorizedBracketPairs": [
        ["[", "]"],
		["(", ")"],
		["{", "}"],
		["<", ">"]
    ],
    "editor.parameterHints.cycle": true,
    "editor.quickSuggestions": {
        "other": "on",
        "comments": "off",
        "strings": "on"
    },
    "editor.semanticHighlighting.enabled": true,
    "editor.stickyScroll.enabled": true,
    "editor.suggest.insertMode": "replace",
    "editor.suggest.localityBonus": true,
    "editor.suggest.matchOnWordStartOnly": false,
    "editor.suggest.showStatusBar": true,
    "editor.tabCompletion": "onlySnippets",
    "editor.tokenColorCustomizations": {
        "[Material Monokai High Contrast]": {
            "textMateRules": [
                {
                    // Python Function Call
                    "name": "Python Generic Function Call",
                    "scope": "meta.function-call.generic.python",
                    "settings": {
                        "foreground": "#A6E22E",
                        "fontStyle": "italic bold"
                    }
                },
                {
                    // Python Attributes
                    "name": "Python Attributes",
                    "scope": [
                        "meta.attribute.python",
                        "variable.other.property.js"
                    ],
                    "settings": {
                        "foreground": "#1EDBEC",
                        "fontStyle": "underline"
                    }
                },
                {
                    // Python Class Name
                    "name": "Python Class Name",
                    "scope": "entity.name.type.class.python",
                    "settings": {
                        "foreground": "#9DFF00",
                        "fontStyle": ""
                    }
                },
                {
                    // Using parameters in functions
                    "name": "Using Paramters",
                    "scope": [
                        "parameter",
                        "variable.parameter",
                        "variable.parameter.js",
                    ],
                    "settings": {
                        "foreground": "#FD971F",
                        "fontStyle": "italic"
                    }
                },
                // {
                // 	// All CAPS Constant
                // 	"scope": "constant.other.caps.python",
                // 	"settings": {
                // 		"foreground": "#4FC1FF"
                // 	}
                // },
            ],
            // "variables": "#ff8c00"
        },
        "[Monokai]": {
            "textMateRules": [
                {
                    // All CAPS Constant
                    "scope": "constant.other.caps.python",
                    "settings": {
                        "foreground": "#B78AFF"
                    }
                },
                {
                    // All CAPS Constant
                    "scope": "variable.other.constant",
                    "settings": {
                        "foreground": "#4FC1FF"
                    }
                },
                {
                    // Python Attributes
                    "scope": "meta.attribute.python",
                    "settings": {
                        "foreground": "#1EDBEC",
                        "fontStyle": "underline"
                    }
                }
            ]
        },
        // https://github.com/doki-theme/doki-theme-vscode/blob/master/package.json#L1135
        // Doki Theme: TypeMoon: Ishtar
        "[62a4f26f-34b2-46f8-a10c-798e48c1ce9d]": {
            "comments": "#909090",
            "keywords": "#F92672",
            "textMateRules": [
                {
                    "name": "Class Names",
                    "scope": "entity.name.type.class",
                    "settings": {
                        "foreground": "#52c3f0",
                        "fontStyle": "bold"
                    }
                },
                {
                    "name": "Python Function Name",
                    "scope": [
                        // "entity.name.function.python",
                        // "Color theme: function.declaration",
                        // "entity.name.function",
                        // "meta.function.python"
                    ],
                    "settings": {
                        "foreground": "#A6E22E",
                        "fontStyle": ""
                    }
                },
                {
                    "name": "Function Parameter",
                    "scope": "variable.parameter",
                    "settings": {
                        "foreground": "#fe73bd",
                        "fontStyle": ""
                    }
                },
                {
                    "scope": [
                        "string.quoted.single.python",
                    ],
                    "settings": {
                        "foreground": "#ffda52",
                        "fontStyle": ""
                    }
                },
                {
                    "scope": "meta.function-call.arguments.python",
                    "settings": {
                        "foreground": "#d7d7d7",
                        "fontStyle": ""
                    }
                },
                {
                    "scope": "string.quoted.docstring.multi.python",
                    "settings": {
                        "foreground": "#fff89a",
                        "fontStyle": "bold"
                    }
                }
            ],
            "functions": "#89ff29"
        }

    },
    "editor.wordWrap": "on",
    "explorer.confirmUndo": "verbose",


    "files.associations": {
        ".env": "env"
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,

    "git.autofetch": true,
    "git.supportCancellation": true,
    "git.showPushSuccessNotification": true,
    "git.statusLimit": 1000,
    "github.gitAuthentication": false,
    "gitlens.codeLens.scopes": [
        "document",
        "containers",
        "blocks"
    ],

    "html.customData": [
        "./.vscode/html-data.json"
    ],
    "html.format.indentHandlebars": true,
    "html.format.indentInnerHtml": true,

    "javascript.inlayHints.enumMemberValues.enabled": true,

    "material-icon-theme.folders.associations": {
        // "serializer": "serializer",
        // "serializers": "serializer"
        "preference": "config",
        "preferences": "config"
    },

    "python.analysis.autoImportCompletions": true,
    "python.analysis.completeFunctionParens": true,
    "python.analysis.indexing": true,
    "python.formatting.provider": "black",
    "python.languageServer": "Pylance",
    "python.linting.enabled": true,
    "python.linting.lintOnSave": true,
    "python.linting.pycodestyleArgs": [
        "--ignore=E501"
    ],
    // "python.linting.pylintArgs": [
    // 	"--load-plugin=pylint_django"
    // ],
    "python.linting.pylintCategorySeverity.warning": "Information",
    "python.linting.pylintEnabled": true,

    "redhat.telemetry.enabled": false,

    "telemetry.telemetryLevel": "off",
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
    "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
    "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
    "typescript.updateImportsOnFileMove.enabled": "always",

