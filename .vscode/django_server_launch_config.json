	"version": "0.2.0",
	"configurations": [
		{
			"name": "Celery APP + BEAT",
			"type": "python",
			"request": "launch",
			"module": "celery",
			"cwd": "${workspaceFolder}",
			"args": [
				"-A",
				"app.app_celery",
				"worker",
				"--beat",
				"--scheduler",
				"django_celery_beat.schedulers:DatabaseScheduler",
				"--loglevel=INFO",
				"--concurrency=8",
				"-O",
				"fair",
				"-P",
				"prefork",
				"-n",
				"cel_app_worker"
			]
		},
		{
			"name": "Django WAN Server",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/manage.py",
			"args": [
				"runserver",
				"0.0.0.0:8000"
			],
			"django": true,
			"justMyCode": false
		},
		{
			"name": "Django Local Server",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/manage.py",
			"args": [
				"runserver",
				"0.0.0.0:8000"
			],
			"django": true,
			"justMyCode": false
		}
	]
}
